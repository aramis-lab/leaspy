[distutils]
index-servers =
    gitlab
    pypi
    testpypi

[gitlab]
repository = https://gitlab.com/api/v4/projects/${CI_PROJECT_ID}/packages/pypi
username = gitlab-ci-token
password = ${CI_JOB_TOKEN}

[pypi]
username = __token__
password = ${PYPI_TOKEN}

[testpypi]
username = __token__
password = ${TESTPYPI_TOKEN}
